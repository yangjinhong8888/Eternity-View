{
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindConfig": "./tailwind.config.ts",
  "tailwindFunctions": ["clsx", "cn"],
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "useTabs": false,
  "quoteProps": "as-needed",
  "endOfLine": "auto"
}
